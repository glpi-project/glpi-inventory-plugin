{
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.83",
        "glpi-project/tools": "^0.7",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.6"
    },
    "conflict": {
        "psr/container": "^2.0"
    },
    "autoload": {
        "classmap": [
            "inc/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.0"
        },
        "sort-packages": true
    }
}
