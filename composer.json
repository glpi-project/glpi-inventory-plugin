{
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.83",
        "glpi-project/tools": "^0.8.0",
        "phpunit/phpunit": "^11"
    },
    "conflict": {
        "psr/container": "^2.0"
    },
    "autoload": {
        "classmap": [
            "inc/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.99"
        },
        "sort-packages": true
    }
}
